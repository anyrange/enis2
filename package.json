{
  "name": "enis2",
  "version": "0.0.0",
  "description": "Convenient, fast, adaptive client for e-journal used in NIS",
  "license": "MIT",
  "scripts": {
    "start": "pnpm --stream -r start",
    "dev": "stale-dep && pnpm --stream -r run dev",
    "build": "stale-dep && pnpm --stream -r run build",
    "prettier": "prettier --check .",
    "lint": "eslint . --ext .js,.vue --ignore-path .gitignore",
    "postinstall": "npx only-allow pnpm && stale-dep -u && pnpm --filter shared run build"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": ">=16.14.0"
  },
  "devDependencies": {
    "stale-dep": "^0.6.0"
  }
}
