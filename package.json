{
  "name": "enis2",
  "version": "1.9.0",
  "license": "MIT",
  "scripts": {
    "dev:client": "vite --config vite.config.client.js",
    "dev:server": "vite --config vite.config.server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "vite build --config vite.config.client.js",
    "build:server": "vite build --config vite.config.server.js --out-dir server/build && NODE_ENV=production esbuild server/build/index.js --platform=node --bundle --minify --target=node16.14.0 --allow-overwrite --outfile=server/dist/index.js && rm -rf server/build",
    "build": "npm run build:client && npm run build:server",
    "start": "concurrently \"vite preview --config vite.config.client.js\" \"node server/dist\"",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "format": "prettier --write .",
    "heroku-postbuild": "echo Skip builds on Heroku"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.38",
    "fastify": "^3.28.0",
    "fastify-compress": "^4.0.1",
    "fastify-cors": "^6.0.3",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^3.0.1",
    "fastify-swagger": "^4.17.1",
    "maska": "^1.5.0",
    "nanoevents": "^6.0.2",
    "nanoid": "^3.3.2",
    "node-fetch": "^2.6.7",
    "pinia": "^2.0.13",
    "v-wave": "^1.4.0",
    "vue": "^3.2.32",
    "vue-scroll-picker": "^1.1.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.173",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.2",
    "vite-plugin-node": "^0.0.19",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-windicss": "^1.8.4",
    "vue-eslint-parser": "^8.3.0",
    "windicss": "^3.5.1",
    "workbox-window": "^6.5.3"
  }
}
